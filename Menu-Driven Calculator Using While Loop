#include <stdio.h>
int main() {
 int ch, a, b;
 while(1) {
 printf("1.Add 2.Subtract 3.Multiply 4.Divide 5.Exit\n");
 scanf("%d", &ch);
 if(ch == 5) break;
 scanf("%d%d", &a, &b);
 if(ch == 1) printf("Sum=%d\n", a+b);
 else if(ch == 2) printf("Diff=%d\n", a-b);
 else if(ch == 3) printf("Prod=%d\n", a*b);
 else if(ch == 4) printf("Div=%d\n", a/b);
 else printf("Invalid Choice\n");
 }
 return 0;
}
